<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <title>CEWW Internals</title>
    <parser>
        <target>docs/api/phpdoc</target>
        <encoding>utf8</encoding>
        <markers>
            <item>TODO</item>
            <item>FIXME</item>
        </markers>
        <extensions>
            <extension>php</extension>
            <extension>js</extension>
        </extensions>
        <visibility>public,protected</visibility>
    </parser>
    <transformer>
        <target>docs/api/phpdoc</target>
    </transformer>
    <logging>
        <level>warn</level>
        <paths>
            <default>docs/api/phpdoc/phpdoc.log</default>
        </paths>
    </logging>
    <transformations>
        <template name="clean" />
    </transformations>
    <files>
        <directory>src</directory>
        <ignore>Tests/*</ignore>
    </files>
</phpdoc>