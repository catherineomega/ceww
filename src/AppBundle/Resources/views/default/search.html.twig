{% extends 'base.html.twig' %}

{% block title %}Title Search{% endblock %}

{% block pageheader %}
    <h1>Title Search</h1>
    {% include 'AppBundle:misc:search-header.html.twig' with {'results': publications} %}
{% endblock %} 

{% block body %}

    {% include 'AppBundle:misc:search-form.html.twig' with {'search_path': 'search'} %}

    {% if q %}
        {% include 'AppBundle:publication:partial/table.html.twig' %}
    {% endif %}

    {% include 'AppBundle:misc:search-footer.html.twig' with {'results': publications} %}

{% endblock %}
