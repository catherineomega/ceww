{% extends 'base.html.twig' %}

 {% block pageheader %} 
    <h1>PublicationType</h1>
 {% endblock %} 

{% block body %}

    <div class="btn-toolbar">
    <div class="btn-group pull-right">
        <a href="{{ path('publication_type_index') }}" class="btn btn-default">
            <span class="glyphicon glyphicon-arrow-left"></span> Back
        </a>
    </div>
</div>
    <table class="table table-bordered table-condensed table-hover table-striped">
        <tbody>
            <tr>
                <th>Label</th>
            <td>{{ publicationType.label }}</td>
            </tr>
            <tr>
                <th>Description</th>
            <td>{{ publicationType.description }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{% if publicationType.created %}{{ publicationType.created|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Updated</th>
                <td>{% if publicationType.updated %}{{ publicationType.updated|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

{% endblock %}
